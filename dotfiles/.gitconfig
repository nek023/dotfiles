[user]
    name = questbeat
    email = questbeat@gmail.com
[color]
    ui = auto
[core]
    excludesfile = /Users/questbeat/.gitignore_global
    precomposeunicode = true
    editor = vim
[push]
    default = current
[pager]
    log = diff-highlight | less
    show = diff-highlight | less
    diff = diff-highlight | less
[alias]
    co = checkout
    br = branch
    ci = commit
    st = status
    unstage = reset HEAD --
    last = log -1 HEAD
    ignore = "!gi() { curl -L -s https://www.gitignore.io/api/$@ ;}; gi"
[filter "media"]
    required = true
    clean = git media clean %f
    smudge = git media smudge %f
[ghq]
    root = ~/.ghq
    root = ~/.go/src
[filter "lfs"]
    clean = git-lfs clean %f
    smudge = git-lfs smudge %f
    required = true

